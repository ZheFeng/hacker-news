{
  "env": {
    "production": {
      "presets": [
        ["env", {"modules": false}],
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-flow-strip-types",
        ["extensible-destructuring", {"mode": "optout", "impl": "immutable"}]
      ],
    },
    "development": {
      "presets": [
        ["env", {"modules": false}],
        "stage-0",
        "react"
      ],
      "plugins": [
        "react-hot-loader/babel",
        "transform-flow-strip-types",
        ["extensible-destructuring", {"mode": "optout", "impl": "immutable"}]
      ],
    },
    "test": {
      "presets": [
        "env",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-flow-strip-types",
        ["extensible-destructuring", {"mode": "optout", "impl": "immutable"}]
      ],
    }
  }
}
